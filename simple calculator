#include<stdio.h>
int main()
{
char op;
int a,b
printf("enternoperator(+-*/):");
scanf("%c",&op);
printf("enter two numbers:");
scanf("%d %d",&a,&b);
switch(op){
case'+':printf("result=%d",a +b);break;
case'-':printf("result=%d",a -b);break;
case'*':printf("result=%d",a *b);break;
case'/':printf("result=%d",a /b);break;
default:printf("invalid operator");
}
return 0;
}
